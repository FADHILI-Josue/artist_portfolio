{
  "rewrites": [
    {"source": "/(.*)", "destination": "/"}
  ],
  "images": {
    "sizes": [256, 640, 1080, 2048, 3840],
    "formats": ["image/webp", "image/jpeg", "image/png"],
    "minimumCacheTTL": 60,
    "dangerouslyAllowSVG": false,
    "contentSecurityPolicy": "script-src 'none'; frame-src 'none'; sandbox;",
    "contentDispositionType": "inline"
  }
}
